{
  "name": "frontend-technical-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npm install --package-lock-only --ignore-scripts && npx npm-force-resolutions",
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start-server": "json-server --watch src/server/db.json --routes src/server/routes.json --middlewares src/server/middleware/conversations.js --port 3005",
    "start-server:prod": "node src/server/server.js",
    "test": "jest",
    "lint": "next lint",
    "lint:css": "stylelint './src/**/*.style.ts'",
    "lint:fix": "eslint . --fix",
    "check": "tsc --noEmit && stylelint './src/**/*.style.ts' && eslint . && prettier --write . && jest",
    "check:fix": "tsc --noEmit && stylelint './src/**/*.style.ts' && eslint . --fix && prettier --write . && jest"
  },
  "engines": {
    "node": ">=14.17.2"
  },
  "resolutions": {
    "@types/react": "^17.0.38"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "date-fns": "^2.28.0",
    "next": "^12.1.5",
    "next-i18next": "^11.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "styled-components": "^5.3.5",
    "styled-reset": "^4.3.4"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.1.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/node": "^16.6.1",
    "@types/react": "^17.0.18",
    "@types/react-dom": "^18.0.1",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "babel-jest": "^27.0.6",
    "babel-plugin-styled-components": "^2.0.7",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "jest-styled-components": "^7.0.8",
    "json-server": "^0.16.3",
    "prettier": "^2.6.2",
    "prettier-eslint": "^13.0.0",
    "stylelint": "^14.7.1",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
